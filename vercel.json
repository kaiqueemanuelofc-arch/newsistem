{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static-build"
    },
    {
      "src": "src/**",
      "use": "@vercel/static-build"
    },
    {
      "src": "style/**",
      "use": "@vercel/static-build"
    },
    {
      "src": "utils/**",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/styles/(.*)",
      "dest": "/style/$1"
    },
    {
      "src": "/src/(.*)",
      "dest": "/src/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
